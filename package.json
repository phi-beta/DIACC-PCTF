{
  "name": "diacc-pctf",
  "version": "1.0.0",
  "description": "Implementation of DIACC Pan-Canadian Trust Framework (PCTF) criteria and components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "demo": "ts-node src/run-demo.ts",
    "start": "node dist/index.js",
    "clean": "rmdir /s /q dist 2>nul || true",
    "rebuild": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["DIACC", "PCTF", "digital-identity", "trust-framework", "authentication", "privacy", "digital-wallet"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
